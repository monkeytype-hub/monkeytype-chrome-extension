{
    "name": "MonkeyType Extension",
    "manifest_version": 3,
    "version": "1.2.0",
    "description": "Help you type faster and more accurately in MonkeyType",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage"
    ]
}
